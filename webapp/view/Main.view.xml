<mvc:View
  controllerName="com.seeql.controller.Main"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns="sap.m">

  <App id="app">
    <pages>
      <Page title="Main">
        <content>

          <!-- GenericTile Ã  la place du bouton -->
          <GenericTile
            class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
            header="Voir seeql"
            subheader="Application IFrame"
            press="onPressIframe"
            frameType="OneByHalf">

            <TileContent footer="Cliquer pour ouvrir">
              <!-- Optionnel : ajouter une info visuelle -->
            </TileContent>
          </GenericTile>

          <GenericTile
            class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
            header="Voir seeql-SAPUI5"
            subheader="Application SAPUI5"
            press="onPressseeqlSAP"
            frameType="OneByHalf">

            <TileContent footer="Cliquer pour ouvrir">
              <!-- Optionnel : ajouter une info visuelle -->
            </TileContent>
          </GenericTile>

        </content>
      </Page>
    </pages>
  </App>
</mvc:View>
